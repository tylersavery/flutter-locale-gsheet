{
	"name": "flutter-locale-gsheet",
	"displayName": "FlutterLocaleGSheet",
	"publisher": "TheYoungAstronauts",
	"description": "",
	"version": "0.0.6",
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:flutter-locale-gsheet.FlutterLocaleGSheet"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "ExtensionSettings",
			"properties": {
				"flutteri18n.sheetId": {
					"type": [
						"string",
						"null"
					],
					"default": "hello",
					"description": "The id of the google sheet."
				},
				"flutteri18n.email": {
					"type": [
						"string",
						"null"
					],
					"default": null,
					"description": "The service account's email address."
				},
				"flutteri18n.privateKey": {
					"type": [
						"string",
						"null"
					],
					"default": null,
					"description": "The service account's private key."
				}
			}
		},
		"commands": [
			{
				"command": "flutter-locale-gsheet.FlutterLocaleGSheet",
				"title": "FlutterLocaleGSheet"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.52.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.15.0",
		"@typescript-eslint/eslint-plugin": "^4.9.0",
		"@typescript-eslint/parser": "^4.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1"
	},
	"dependencies": {
		"balanced-match": "^1.0.0",
		"escape-string-regexp": "^4.0.0",
		"google-spreadsheet": "^3.1.15"
	}
}